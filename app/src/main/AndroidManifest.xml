<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sylvan.myworkdemo">

    <application
        android:name=".MyWorkDemoApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".AnimationActivity"/>

        <activity android:name=".pip.PipViewActivity"/>

        <activity android:name=".MarqueeActivity"/>
        <service android:name=".pip.FloatVideoService"/>

        <activity android:name=".RecyclerViewActivity"/>

        <activity android:name=".ProgressActivity"/>

        <activity android:name=".MyTrapezoidActivity"/>

        <activity android:name=".MyRatingBarAct"/>

        <activity android:name=".ImagePickActivity"/>

    </application>

</manifest>