<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <!--视频悬浮窗-->
    <style name="FloatWindowAnimation">
        <item name="android:windowEnterAnimation">@anim/anim_float_window_enter</item>
        <item name="android:windowExitAnimation">@anim/anim_float_window_exit</item>
    </style>

    <style name="RatingBarStyle" parent="@style/Widget.AppCompat.RatingBar">
        <item name="android:progressBarStyle">@drawable/ratingbar_full</item>
        <!--<item name="android:minHeight">24dp</item>-->
        <!--<item name="android:maxHeight">24dp</item>-->
    </style>

</resources>
